            android:id="@+id/action_splash_to_privacy_fragment"
            app:destination="@id/privacy_fragment"
            app:launchSingleTop="true" />
        <action
            android:id="@+id/action_splash_to_terms_fragment"
            app:destination="@id/terms_fragment"
            app:launchSingleTop="true" />
    </fragment>
    <fragment
