            android:id="@+id/action_splash_to_subscr_fragment"
            app:destination="@id/subscr_fragment"
            app:launchSingleTop="true" />
        <action
            android:id="@+id/action_splash_to_support_fragment"
            app:destination="@id/support_fragment"
