            android:id="@+id/action_splash_to_delete_ac_fragment"
            app:destination="@id/delete_ac_fragment"
            app:launchSingleTop="true" />
        <action
            android:id="@+id/action_splash_to_change_pin_fragment"
            app:destination="@id/change_pin_fragment"
            app:launchSingleTop="true" />
    </fragment>
    <fragment
